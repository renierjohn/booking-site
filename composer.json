{
    "name": "renierjohn/booking-site",
    "description": "A recipe tailored to best practices for kickstarting a new Drupal 10 project.",
    "keywords": ["recipe", "Drupal base install", "best practices", "simple install"],
    "type": "drupal-recipe",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Renify",
            "homepage": "https://github.com/renierjohn",
            "role": "Maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "installer-paths": {
        "web/libraries/{$name}": [
            "type:drupal-library",
            "type:bower-asset",
            "type:npm-asset"
        ]
    },
    "installer-types": ["bower-asset", "npm-asset"],
    "require": {
        "drupal/bm":"1.0.x-dev@dev",
        "drupal/select2":"^1.15",
        "drupal/field_group": "^3.4",
        "select2/select2": "^4.0"
    }
}
